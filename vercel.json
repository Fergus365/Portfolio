{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=63072000; includeSubDomains; preload"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self'; font-src 'self'; frame-src 'none'; object-src 'none'; base-uri 'self'; form-action 'self'"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "http://kristofferkarlsson.se/:path*",
            "destination": "https://kristofferkarlsson.se/:path*",
            "permanent": true
        }
    ]
}